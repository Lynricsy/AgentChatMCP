{
  "name": "telegram-feedback-mcp",
  "version": "0.2.0",
  "description": "基于 Telegram 的交互式反馈 MCP：让 AI 随时发问并等待用户回复（含图片）",
  "type": "module",
  "bin": {
    "telegram-feedback-mcp": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "prepare": "npm run build",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "axios": "^1.13.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "mcp",
    "telegram",
    "feedback",
    "interactive"
  ],
  "license": "MIT"
}
