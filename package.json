  "devDependencies": {
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "mcp",
    "telegram",
    "agentchat",
    "agent",
    "chat",
    "ai",
    "feedback",
    "interactive"
  ],
